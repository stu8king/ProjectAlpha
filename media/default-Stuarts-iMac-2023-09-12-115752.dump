INSERT INTO "tblThreatIntelligence" VALUES(1,'Advanced Persistent Threat (APT)','Zero-day exploits, spear phishing, watering hole attacks','Industrial espionage, political motives, financial gain','Data exfiltration, intellectual property theft, reputational damage');
INSERT INTO "tblThreatIntelligence" VALUES(2,'Distributed Denial of Service (DDoS)','Botnets, amplification attacks, application layer attacks','Hacktivism, revenge, competition sabotage','Service disruption, operational downtime, financial losses');
INSERT INTO "tblThreatIntelligence" VALUES(3,'Social Engineering','Phishing, pretexting, baiting','Identity theft, unauthorized access, fraud','Account compromise, data breaches, reputational damage');
INSERT INTO "tblThreatIntelligence" VALUES(4,'Insider Threats','Data theft, sabotage, unauthorized access','Revenge, financial gain, espionage','Data leaks, intellectual property theft, reputational damage');
INSERT INTO "tblThreatIntelligence" VALUES(5,'Malware','Drive-by downloads, email attachments, malicious websites','Financial gain, data destruction, espionage','System compromise, data breaches, operational disruption');
INSERT INTO "tblThreatIntelligence" VALUES(6,'Phishing','Spear phishing, deceptive websites, social engineering','Credential theft, fraud, identity impersonation','Account compromise, data breaches, financial losses');
INSERT INTO "tblThreatIntelligence" VALUES(7,'Ransomware','Email attachments, exploit kits, remote desktop protocol (RDP) compromise','Monetary extortion, disruption, sabotage','Data encryption, operational downtime, financial losses');
INSERT INTO "tblThreatIntelligence" VALUES(8,'Zero-Day Exploits','Undisclosed vulnerabilities, targeted attacks','Financial gain, espionage, competitive advantage','System compromise, data breaches, reputational damage');
INSERT INTO "tblThreatIntelligence" VALUES(9,'SQL Injection','Malicious input, vulnerable web applications','Data theft, unauthorized access, data manipulation','Data breaches, information disclosure, reputational damage');
INSERT INTO "tblThreatIntelligence" VALUES(10,'Pharming','DNS hijacking, malicious website redirection','Identity theft, financial fraud, data interception','Phishing attacks, account compromise, financial losses');
INSERT INTO "tblThreatIntelligence" VALUES(11,'Insufficient Authentication','Weak passwords, default credentials, single-factor authentication','Unauthorized access, privilege escalation, account compromise','Data breaches, unauthorized actions, system compromise');
INSERT INTO "tblThreatIntelligence" VALUES(12,'Man-in-the-Middle (MitM) Attacks','Network interception, session hijacking, SSL/TLS vulnerabilities','Data interception, unauthorized access, identity theft','Data manipulation, account compromise, privacy breaches');
INSERT INTO "tblThreatIntelligence" VALUES(13,'Cross-Site Scripting (XSS)','Injection of malicious scripts into web pages','Data theft, session hijacking, phishing attacks','Information disclosure, account compromise, reputational damage');
INSERT INTO "tblThreatIntelligence" VALUES(14,'Brute Force Attacks','Repeated login attempts, password guessing','Account compromise, unauthorized access, data theft','Account lockouts, data breaches, security vulnerabilities');
INSERT INTO "tblThreatIntelligence" VALUES(15,'Credential Stuffing','Automated login attempts using stolen credentials','Account takeover, identity theft, fraud','Account compromise, unauthorized access, reputational damage');
INSERT INTO "tblThreatIntelligence" VALUES(16,'Supply Chain Attacks','Compromised software, hardware, or firmware','Espionage, data theft, system compromise','Data breaches, system compromise, reputational damage');
INSERT INTO "tblThreatIntelligence" VALUES(17,'Phishing Emails','Spoofed email addresses, deceptive content','Credential theft, data breaches, fraud','Account compromise, data loss, reputational damage');
INSERT INTO "tblThreatIntelligence" VALUES(18,'Malicious Insider Activities','Unauthorized access, data exfiltration, sabotage','Revenge, financial gain, espionage','Data breaches, system compromise, reputational damage');
INSERT INTO "tblThreatIntelligence" VALUES(19,'Denial-of-Service (DoS) Attacks','Flooding target with excessive requests or traffic','Service disruption, competitive advantage','Operational downtime, financial losses, reputational damage');
INSERT INTO "tblThreatIntelligence" VALUES(20,'Web Application Vulnerabilities','Injection attacks, insecure coding practices','Data theft, unauthorized access, system compromise','Data breaches, information disclosure, reputational damage');
INSERT INTO "tblThreatIntelligence" VALUES(21,'Data Exfiltration','Unauthorized data transfer, network exfiltration','Intellectual property theft, competitive advantage','Data loss, reputational damage, legal implications');
INSERT INTO "tblThreatIntelligence" VALUES(22,'Insider Data Theft','Unauthorized access, data exfiltration','Financial gain, espionage, sabotage','Data breaches, reputational damage, legal consequences');
INSERT INTO "tblThreatIntelligence" VALUES(23,'Zero-Day Vulnerabilities','Unknown vulnerabilities in software or systems','Exploitation, financial gain, competitive advantage','System compromise, data breaches, operational disruption');
INSERT INTO "tblThreatIntelligence" VALUES(24,'Business Email Compromise (BEC)','Spoofing, social engineering, impersonation','Financial fraud, unauthorized transactions','Financial losses, reputational damage');
INSERT INTO "tblThreatIntelligence" VALUES(25,'Fileless Malware','Memory-based attacks, living-off-the-land techniques','Stealthy persistence, data theft, remote control','System compromise, data breaches, operational disruption');
INSERT INTO "tblThreatIntelligence" VALUES(26,'Cryptojacking','Unauthorized use of computing resources for cryptocurrency mining','Monetary gain, resource exploitation','High CPU usage, reduced system performance, energy costs');
INSERT INTO "tblThreatIntelligence" VALUES(27,'Social Media Attacks','Phishing, account hijacking, clickjacking','Identity theft, unauthorized access, spreading malware','Account compromise, reputational damage, malware distribution');
INSERT INTO "tblThreatIntelligence" VALUES(28,'Wireless Network Attacks','Eavesdropping, man-in-the-middle attacks, rogue access points','Data interception, unauthorized access, network compromise','Data theft, unauthorized access, network disruption');
INSERT INTO "tblThreatIntelligence" VALUES(29,'Data Manipulation','Unauthorized alteration or modification of data','Sabotage, financial fraud, reputational damage','Data integrity loss, operational errors, financial losses');
