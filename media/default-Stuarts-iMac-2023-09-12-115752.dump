CREATE TABLE IF NOT EXISTS tblControlObjectives (ID INTEGER PRIMARY KEY AUTOINCREMENT, ControlObjective TEXT);
INSERT INTO "tblControlObjectives" VALUES(1,'{"name": "Access Control", "description": "Restrict access to information and resources based on user roles and responsibilities."}');
INSERT INTO "tblControlObjectives" VALUES(2,'{"name": "Asset Management", "description": "Maintain an inventory of information assets and track their lifecycle."}');
INSERT INTO "tblControlObjectives" VALUES(3,'{"name": "Change Management", "description": "Establish a formal process for managing changes to systems, applications, and configurations."}');
INSERT INTO "tblControlObjectives" VALUES(4,'{"name": "Data Protection", "description": "Implement encryption mechanisms and data loss prevention controls to protect sensitive data."}');
INSERT INTO "tblControlObjectives" VALUES(5,'{"name": "Incident Response", "description": "Develop an incident response plan and establish procedures for detecting, responding to, and recovering from cybersecurity incidents."}');
INSERT INTO "tblControlObjectives" VALUES(6,'{"name": "Network Security", "description": "Implement firewalls, intrusion detection systems, and network segmentation to protect the network."}');
INSERT INTO "tblControlObjectives" VALUES(7,'{"name": "Physical Security", "description": "Implement physical controls to protect physical assets, data centers, and sensitive areas."}');
INSERT INTO "tblControlObjectives" VALUES(8,'{"name": "Security Awareness and Training", "description": "Provide regular cybersecurity training and awareness programs to employees."}');
INSERT INTO "tblControlObjectives" VALUES(9,'{"name": "Security Monitoring and Logging", "description": "Implement a centralized logging system and monitor logs for suspicious activities."}');
INSERT INTO "tblControlObjectives" VALUES(10,'{"name": "System Hardening and Vulnerability Management", "description": "Regularly update and patch systems, and conduct vulnerability assessments."}');
INSERT INTO "tblControlObjectives" VALUES(11,'{"name": "Supplier and Third-Party Risk Management", "description": "Assess the cybersecurity posture of suppliers and establish contractual agreements."}');
INSERT INTO "tblControlObjectives" VALUES(12,'{"name": "Business Continuity and Disaster Recovery", "description": "Develop and test business continuity and disaster recovery plans."}');
INSERT INTO "tblControlObjectives" VALUES(13,'{"name": "Compliance", "description": "Ensure compliance with relevant laws, regulations, and industry standards."}');
INSERT INTO "tblControlObjectives" VALUES(14,'{"name": "Privacy Protection", "description": "Implement measures to protect personal data and ensure compliance with privacy regulations."}');
INSERT INTO "tblControlObjectives" VALUES(15,'{"name": "Risk Assessment and Management", "description": "Conduct regular risk assessments and implement risk mitigation measures."}');
