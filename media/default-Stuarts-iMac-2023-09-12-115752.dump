INSERT INTO "accounts_subscriptiontype" VALUES(4,'Individual - Monthly','Unlimited assessments for a single user. Automatically renews monthly.  Cancel anytime.  Free read-only access for 12 months after the end of the subscription.',30,0,1,120,'2','30 Days','90 Days','10','10','None','None');
