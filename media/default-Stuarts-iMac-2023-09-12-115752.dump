CREATE TABLE IF NOT EXISTS tblRiskCategories (ID INTEGER PRIMARY KEY AUTOINCREMENT, CategoryName TEXT, Description TEXT);
INSERT INTO "tblRiskCategories" VALUES(1,'Unauthorized Access','Risks related to unauthorized access to systems, networks, or sensitive information.');
INSERT INTO "tblRiskCategories" VALUES(2,'Data Breach and Data Loss','Risks associated with the compromise, loss, or unauthorized disclosure of sensitive data.');
INSERT INTO "tblRiskCategories" VALUES(3,'Malware and Cyberattacks','Risks related to malicious software, viruses, and cyberattacks.');
INSERT INTO "tblRiskCategories" VALUES(4,'System and Network Disruptions','Risks associated with disruptions to systems, networks, or critical services.');
INSERT INTO "tblRiskCategories" VALUES(5,'Insider Threats','Risks posed by authorized individuals with malicious intent or who accidentally compromise security.');
INSERT INTO "tblRiskCategories" VALUES(6,'Social Engineering','Risks associated with manipulative techniques targeting individuals to deceive and gain unauthorized access.');
INSERT INTO "tblRiskCategories" VALUES(7,'Vulnerabilities and Exploits','Risks related to software or system vulnerabilities that can be exploited by attackers.');
INSERT INTO "tblRiskCategories" VALUES(8,'Third-Party and Supply Chain Risks','Risks arising from vulnerabilities or security weaknesses in third-party vendors, suppliers, or business partners.');
INSERT INTO "tblRiskCategories" VALUES(9,'Data Privacy and Compliance','Risks associated with non-compliance with data protection regulations or privacy requirements.');
INSERT INTO "tblRiskCategories" VALUES(10,'Technology Obsolescence and Legacy Systems','Risks arising from outdated or unsupported technology, systems, or software.');
INSERT INTO "tblRiskCategories" VALUES(11,'Lack of Security Awareness and Training','Risks resulting from inadequate knowledge and awareness of cybersecurity best practices.');
INSERT INTO "tblRiskCategories" VALUES(12,'Physical Security Breaches','Risks related to physical security breaches or unauthorized access to physical infrastructure.');
INSERT INTO "tblRiskCategories" VALUES(13,'Compliance Violations','Risks associated with non-compliance with applicable laws, regulations, or industry standards.');
INSERT INTO "tblRiskCategories" VALUES(14,'Business Continuity and Disaster Recovery','Risks related to the inability to recover critical systems and data in the event of a cybersecurity incident or disaster.');
INSERT INTO "tblRiskCategories" VALUES(15,'Intellectual Property Theft','Risks related to the theft or unauthorized use of intellectual property or trade secrets.');
INSERT INTO "tblRiskCategories" VALUES(16,'Cloud Security Risks','Risks associated with the use of cloud services, including data breaches, data leakage, and lack of control over cloud infrastructure.');
INSERT INTO "tblRiskCategories" VALUES(17,'IoT (Internet of Things) Risks','Risks associated with vulnerabilities and security weaknesses in IoT devices and systems.');
INSERT INTO "tblRiskCategories" VALUES(18,'Regulatory and Legal Risks','Risks arising from non-compliance with cybersecurity regulations and legal requirements.');
INSERT INTO "tblRiskCategories" VALUES(19,'Reputation and Brand Damage','Risks related to negative publicity, loss of customer trust, and damage to brand reputation.');
INSERT INTO "tblRiskCategories" VALUES(20,'Lack of Incident Response Readiness','Risks associated with the absence of a well-defined incident response plan or lack of preparedness to handle cybersecurity incidents.');
INSERT INTO "tblRiskCategories" VALUES(21,'Health and Safety',NULL);
