INSERT INTO "accounts_subscriptiontype" VALUES(1,'Organization - 100','12 months unlimited assessments and users. Read-only access for a further 12 months after the end of the subscription period.',365,0,100,12000,'1','1 Year','1 Year','Unlimited','Unlimited','Yes','Yes');
