INSERT INTO "tblMitreICSTactics" VALUES('TA0108','Initial Access','The adversary is trying to get into your ICS environment.');
INSERT INTO "tblMitreICSTactics" VALUES('TA0104','Execution','The adversary is trying to run code or manipulate system functions, parameters, and data in an unauthorized way.');
INSERT INTO "tblMitreICSTactics" VALUES('TA0100','Collection','The adversary is trying to gather data of interest and domain knowledge on your ICS environment to inform their goal.');
INSERT INTO "tblMitreICSTactics" VALUES('TA0101','Command and Control','The adversary is trying to communicate with and control compromised systems, controllers, and platforms with access to your ICS environment.');
INSERT INTO "tblMitreICSTactics" VALUES('TA0102','Discovery','The adversary is locating information to assess and identify their targets in your environment.');
INSERT INTO "tblMitreICSTactics" VALUES('TA0103','Evasion','The adversary is trying to avoid security defenses.');
INSERT INTO "tblMitreICSTactics" VALUES('TA0105','Impact','The adversary is trying to manipulate, interrupt, or destroy your ICS systems, data, and their surrounding environment.');
INSERT INTO "tblMitreICSTactics" VALUES('TA0106','Impair Process Control','The adversary is trying to manipulate, disable, or damage physical control processes.');
INSERT INTO "tblMitreICSTactics" VALUES('TA0107','Inhibit Response Function','The adversary is trying to prevent your safety, protection, quality assurance, and operator intervention functions from responding to a failure, hazard, or unsafe state.');
INSERT INTO "tblMitreICSTactics" VALUES('TA0109','Lateral Movement','The adversary is trying to move through your ICS environment.');
INSERT INTO "tblMitreICSTactics" VALUES('TA0110','Persistence','The adversary is trying to gather data of interest and domain knowledge on your ICS environment to inform their goal.');
INSERT INTO "tblMitreICSTactics" VALUES('TA0111','Privilege Escalation','The adversary is trying to gain higher-level permissions.');
